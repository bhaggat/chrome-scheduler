{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0.0",
    "description": "A Chrome extension built with React and Vite.",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  